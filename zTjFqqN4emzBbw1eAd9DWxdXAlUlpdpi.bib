  author = {Matangila, Junior R. and Mitashi, Patrick and InocÃªncio da Luz, Raquel A. and Lutumba, Pascal T. and Van Geertruyden, Jean-Pierre},
  date = {2015-11-14},
  journaltitle = {Malaria Journal},
  shortjournal = {Malar J},
