  keywords = {artemisinin,artemisinina,artÃ©misinine,children,colegio,Ã©cole,enfants,intermittent preventive treatment,malaria,niÃ±os,PTI,school,TPI},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1365-3156.2009.02294.x},
