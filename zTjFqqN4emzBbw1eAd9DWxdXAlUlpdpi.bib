@article{mbitiEvaluatingCurriculumReforms,
  title = {Evaluating {{Curriculum Reforms}} in {{Developing Countries}}: {{Evidence}} from {{Tanzania}}},
  author = {Mbiti, Isaac and Rodriguez-Segura, Daniel},
  pages = {32},
  abstract = {As a response to the low learning outcomes in even the most basic literacy and numeracy skills across the country, the Tanzanian government implemented in 2015 the â€œ3Râ€ curricular reform - reading, writing, and arithmetic - for grades one and two. This curricular shift officially allocated 80\% of the instructional time in these grades to foundational literacy and numeracy skills, and greatly diminished the focus on tangential subjects that overcrowded the previous curriculum. We evaluate the effect of this policy on learning outcomes, by using panel data from 2013-2016, and a difference-in-differences approach which leverages the variation in the timing of implementation and the cohorts impacted by the policy. We find that the policy had large, significantly positive effects for the average child: a gain of 0.18 standard deviations in Kiswahili and 0.55 standard deviations in math. These effects were augmented when teachers were trained to implement the curriculum in a timely fashion. We hypothesize that the large results stem from a combination of very low initial scores, and a successful simplification of the curriculum which allowed instruction to be focused on the development of foundational skills. The paper contributes to the overall literature on curriculum reform in developing countries, providing a case study on the large-scale implementation and effects on student learning induced by this type of policy.},
  langid = {english},
  file = {C\:\\Users\\dougj\\Zotero\\storage\\HLG446S3\\Mbiti and Rodriguez-Segura - Evaluating Curriculum Reforms in Developing Countr.pdf}
}

