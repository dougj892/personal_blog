@article{opokuImpactCombinedIntermittent2016,
  title = {Impact of Combined Intermittent Preventive Treatment of Malaria and Helminths on Anaemia, Sustained Attention, and Recall in {{Northern Ghanaian}} Schoolchildren},
  author = {Opoku, Ernest Cudjoe and Olsen, Annette and Browne, Edmund and Hodgson, Abraham and Awoonor-Williams, John K. and Yelifari, Lawrence and Williams, John and Magnussen, Pascal},
  date = {2016-12-01},
