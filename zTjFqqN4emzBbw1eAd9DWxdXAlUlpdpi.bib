  title = {Effectiveness of an {{Inactivated SARS}}-{{CoV}}-2 {{Vaccine}} in {{Chile}}},
  author = {Jara, Alejandro and Undurraga, Eduardo A. and GonzÃ¡lez, Cecilia and Paredes, Fabio and Fontecilla, TomÃ¡s and Jara, Gonzalo and Pizarro, Alejandra and Acevedo, Johanna and Leo, Katherine and Leon, Francisco and Sans, Carlos and Leighton, Paulina and SuÃ¡rez, Pamela and GarcÃ­a-Escorza, Heriberto and Araos, Rafael},
  date = {2021-07-07},
