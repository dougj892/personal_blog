@report{piperEffectivenessTeachersGuides2018,
  title = {Effectiveness of {{Teachers}}â€™ {{Guides}} in the {{Global South}}: {{Scripting}}, {{Learning Outcomes}}, and {{Classroom Utilization}}},
  shorttitle = {Effectiveness of {{Teachers}}â€™ {{Guides}} in the {{Global South}}},
  author = {Piper, Benjamin and Sitabkhan, Yasmin and Mejia, Jessica and Betts, Kellie},
  date = {2018-05-11},
  institution = {{RTI Press}},
  doi = {10.3768/rtipress.2018.op.0053.1805},
  url = {https://www.rti.org/rti-press-publication/effectiveness-teachers-guides-global-south},
  urldate = {2019-11-13},
  abstract = {This report presents the results of RTI International Educationâ€™s study on teachers' guides across 13 countries and 19 projects. Using quantitative and qualitative methods, we examine how teachersâ€™ guides across the projects differ and find substantial variation in the design and structure of the documents. We develop a scripting index so that the scripting levels of the guides can be compared across projects. The impact results of the programs that use teachersâ€™ guides show significant impacts on learning outcomes, associated with approximately an additional half year of learning, showing that structured teachersâ€™ guides contribute to improved learning outcomes. During observations, we find that teachers make a variety of changes in their classroom instruction from how the guides are written, showing that the utilization of structured teachersâ€™ guides do not create robotic teachers unable to use their own professional skills to teach children. Unfortunately, many changes that teachers make reduce the amount of group work and interactivity that was described in the guides, suggesting that programs should encourage teachers to more heavily utilize the instructional routines designed in the guide. The report includes a set of research-based guidelines that material developers can use to develop teachersâ€™ guides that will support effective instructional practices and help improve learning outcomes. The key takeaway from the report is that structured teachers' guides improve learning outcomes, but that overly scripted teachers' guides are somewhat less effective than simplified teachers' guides that give specific guidance to the teacher but are not written word for word for each lesson in the guide.},
  langid = {english},
  file = {C\:\\Users\\dougj\\Zotero\\storage\\T3ZZG35D\\Piper et al. - 2018 - Effectiveness of Teachersâ€™ Guides in the Global So.pdf}
