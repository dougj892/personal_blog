@article{graisMolecularMarkersResistance2018,
  title = {Molecular Markers of Resistance to Amodiaquine plus Sulfadoxineâ€“Pyrimethamine in an Area with Seasonal Malaria Chemoprevention in South Central {{Niger}}},
  author = {Grais, Rebecca F. and Laminou, Ibrahim M. and Woi-Messe, Lynda and Makarimi, Rockyath and Bouriema, Seidou H. and Langendorf, Celine and Amambua-Ngwa, Alfred and Dâ€™Alessandro, Umberto and GuÃ©rin, Philippe J. and Fandeur, Thierry and Sibley, Carol H.},
  date = {2018-12},
  journaltitle = {Malaria Journal},
  shortjournal = {Malar J},
